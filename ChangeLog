	    "", $4); gsub(/^\n*/, "", $4); gsub(/\n/, ", ", $4);
	    gsub(/\ngit-svn-id.*/, "", $3); gsub(/\n/, "\n\t  ", $3); print
	    $1 " " $4 ": "; print "\t  " $2 $3 }}' | \ fmt --width=78 -c
	    > ChangeLog

